    "axios": "^1.2.3",
    "blob-stream": "^0.1.3",
    "chart.js": "^4.2.0",
    "is-dark-color": "^1.2.0",
    "md5": "^2.3.0",
    "moment": "^2.29.4",
    "pinia": "^2.0.29",
    "qr-code-styling": "^1.6.0-rc.1",
    "socket.io-client": "^4.5.4",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
